    <Exception>
      <Format>application/vnd.ogc.se_xml</Format>
    </Exception>
    <UserDefinedSymbolization SupportSLD="1" UserLayer="1" UserStyle="1" RemoteWFS="1"/>
    <Layer>
      <Title>GeoServer Web Map Service</Title>
      <Abstract>A compliant implementation of WMS 1.1.1 plus most of the SLD 1.0 extension (dynamic styling). Can also generate PDF, SVG, KML, GeoRSS</Abstract>
      <SRS>EPSG:2400</SRS>
      <LatLonBoundingBox minx="-180.0" miny="-90.0" maxx="180.0" maxy="90.0"/>
      <Layer queryable="1">
        <Name>BK:deposits</Name>
        <Title>Gruvor</Title>
        <Abstract>Generated from gruvor</Abstract>
        <KeywordList>
          <Keyword>gruvor</Keyword>
        </KeywordList>
        <SRS>EPSG:2400</SRS>
        <LatLonBoundingBox minx="13.700304542943266" miny="57.62990474998256" maxx="20.60183025896277" maxy="64.02089042528179"/>
        <BoundingBox SRS="EPSG:2400" minx="1380150.5" miny="6360984.0" maxx="1751939.5" maxy="7136836.0"/>
        <Style>
          <Name>point</Name>
          <Title>Default point</Title>
          <Abstract>A sample style that just prints out a 6px wide red square</Abstract>
          <LegendURL width="20" height="20">
            <Format>image/png</Format>
            <OnlineResource xmlns:xlink="http://www.w3.org/1999/xlink" xlink:type="simple" xlink:href="http://localhost:8080/geoserver/wms/GetLegendGraphic?VERSION=1.0.0&amp;FORMAT=image/png&amp;WIDTH=20&amp;HEIGHT=20&amp;LAYER=topp:gruvor"/>
          </LegendURL>
        </Style>
      </Layer>
    </Layer>
